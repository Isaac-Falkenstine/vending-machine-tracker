<h1> <%=  @snack.name %> </h1>
<h3> Price: <%=  @snack.price %> </h3>

<h4>Locations:</h4>
<% @snack.machines.each do |machine| %>
   <p><h4>Machine:</h4> <%=  machine.location %></p>
   <p>Average Snack Cost: <%= machine.average_snack_price %></p>
   <p>Number of Snacks: <%= machine.snacks.count %></p>
<% end %>
